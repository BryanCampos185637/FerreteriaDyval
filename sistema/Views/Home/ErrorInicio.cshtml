
@{
    ViewData["Title"] = "ErrorInicio";
}

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 mt-5">
        <div class="card">
            <div class="card-header bg-dark">
                <div class="text-center">
                    <h1 class="text-light">Error 401</h1>
                </div>
            </div>
            <div class="card-body text-center">
                <h3>@ViewBag.cod <i class="fas fa-sad-cry"></i></h3>
                <h5>@ViewBag.error</h5>
                @*<p>Lamentamos el inconveniente, si deseas acceder a la página comunicate con un administrador del sistema.</p>*@
            </div>
            <div class="card-footer">
                <div class="text-center">
                    <a asp-action="Index" asp-controller="Login" class="btn btn-primary">Intentar de nuevo</a>
                </div>
            </div>
        </div>
    </div>
</div>


